<h1> Index page </h1>
<p><%= current_user.id %><p>
<p><%= current_user.email %><p>

<h3> current posts: </h3>
<ol>
    <% @posts.each do | p | %>
        <li> <%= p.title %> </li>
        <h4> Author: <%= User.find_by(id: p.user_id).email if user_signed_in? %> </h4>
        <p> <%= p.body%> </p>
    <% end %>
</ol>